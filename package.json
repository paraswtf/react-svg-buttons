  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paraswtf/react-svg-buttons.git"
  },
  "bugs": {
    "url": "https://github.com/paraswtf/react-svg-buttons/issues"
  },
  "scripts": {
    "build": "set NODE_ENV=production rm -rf lib/* && babel src --no-comments --out-dir lib",
    "watch": "set NODE_ENV=development babel src --out-dir lib --watch",
    "build:demo-lib:watch": "set NODE_ENV=development babel src --out-dir demo/src/lib --watch",
    "format": "prettier --tab-width=4 --bracket-spacing --no-semi --trailing-comma es5 --single-quote --write 'src/**/*.js' 'demo/src/**/*.js'",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "react-motion": "^0.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^4.7.16",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.11.0",
    "prettier": "^1.5.3",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "keywords": [
    "react",
    "react-component",
    "icon",
    "button",
    "ui"
  ],
  "homepage": "https://github.com/paraswtf/react-svg-buttons#readme"
