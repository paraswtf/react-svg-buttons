{
  "name": "@paraswtf/react-svg-buttons",
  "description": "Configurable animated SVG buttons for react, fork of react-svg-buttons",
  "version": "0.4.5",
  "main": "lib",
  "author": "RaphaÃ«l Benitte",
  "maintainers": [
    "plouc",
